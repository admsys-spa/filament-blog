{
    "name": "admsys/filament-blog",
    "type": "library",
    "description": "blog package for Filament Admin Panel",
    "keywords": [
        "filament",
        "laravel",
        "blog",
        "admsys"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Díaz Márquez",
            "email": "alexander@admsys.cl",
            "homepage": "https://github.com/admsys-spa",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "filament/support": "^3.2",
        "awcodes/filament-tiptap-editor": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Admsys\\FilamentBlog\\": "src/",
            "Admsys\\FilamentBlog\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Admsys\\FilamentBlog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Admsys\\FilamentBlog\\FilamentBlogServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9",
        "pestphp/pest": "2.34.5",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
